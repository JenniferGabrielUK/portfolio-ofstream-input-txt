#include <iostream>
#include <fstream>
#include <iomanip>

int main () {
	std::ofstream out_file{ "output_1.txt", std::ios::app };
	if (!out_file) {
		std::cerr << "Error creating file." << std::endl;
		std::cerr << "Error creating file." << std::endl;
		return 1;
	}
  	std::string line;
	std::cout << "Enter data for output write to file: " << std::endl;
	std::getline(std::cin,line);
	out_file << line << std::endl;
return 0;
}
